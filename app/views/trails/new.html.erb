<%= form_for([@user, @trail]) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :image_path %>
  <%= f.text_field :image_path %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :tag %>

  <%= f.submit %>

<% end %>
