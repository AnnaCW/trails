<h1>Login</h1>

<div class = "container">

<%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Login" %>

<% end %>

</div>
